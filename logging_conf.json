{
            "version": 1,
            "disable_existing_loggers": false,
            "handlers": {
                "console": {
                    "class": "logging.StreamHandler",
                    "level": "DEBUG",
                    "formatter": "detailed",
                    "stream": "ext://sys.stderr"
                },
                "file": {
                    "class": "logging.handlers.RotatingFileHandler",
                    "level": "DEBUG",
                    "formatter": "detailed",
                    "filename": "./log/phenomizer.LOG",
                    "mode": "a",
                    "maxBytes": 10485760,
                    "backupCount": 20
                }
            },
            "formatters": {
                "detailed": {
                    "format": "%(name)-12s: %(asctime)-15s - %(levelname)s - %(filename)s[line:%(lineno)d process(%(processName)s):%(process)d thread(%(threadName)s):%(thread)d] - %(message)s"
                }
            },
            "loggers": {
                "": {
                    "level": "DEBUG",
                    "handlers": ["file", "console"]
                }
            }
}
